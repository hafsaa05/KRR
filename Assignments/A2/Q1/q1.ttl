###  http://www.semanticweb.org/hafsa/ontologies/2025/10/wound-ontology/Cause
:Cause rdf:type owl:Class .


###  http://www.semanticweb.org/hafsa/ontologies/2025/10/wound-ontology/ChemicalExposure
:ChemicalExposure rdf:type owl:Class ;
                  rdfs:subClassOf :Cause ;
                  owl:disjointWith :Infection ,
                                   :PhysicalInjury .


###  http://www.semanticweb.org/hafsa/ontologies/2025/10/wound-ontology/ChronicWound
:ChronicWound rdf:type owl:Class ;
              rdfs:subClassOf :Wound ;
              owl:disjointWith :SurgicalWound .


###  http://www.semanticweb.org/hafsa/ontologies/2025/10/wound-ontology/CleaningCare
:CleaningCare rdf:type owl:Class ;
              rdfs:subClassOf :Treatment ;
              owl:disjointWith :DressingCare ,
                               :SurgicalCare .


###  http://www.semanticweb.org/hafsa/ontologies/2025/10/wound-ontology/DressingCare
:DressingCare rdf:type owl:Class ;
              rdfs:subClassOf :Treatment ;
              owl:disjointWith :SurgicalCare .


###  http://www.semanticweb.org/hafsa/ontologies/2025/10/wound-ontology/ExistingCondition
:ExistingCondition rdf:type owl:Class ;
                   rdfs:subClassOf :Patient ;
                   owl:disjointWith :NewPatient .


###  http://www.semanticweb.org/hafsa/ontologies/2025/10/wound-ontology/HealingStage
:HealingStage rdf:type owl:Class ;
              rdfs:subClassOf [ rdf:type owl:Restriction ;
                                owl:onProperty :treatedBy ;
                                owl:someValuesFrom :Treatment
                              ] .


###  http://www.semanticweb.org/hafsa/ontologies/2025/10/wound-ontology/Infection
:Infection rdf:type owl:Class ;
           rdfs:subClassOf :Cause ;
           owl:disjointWith :PhysicalInjury .


###  http://www.semanticweb.org/hafsa/ontologies/2025/10/wound-ontology/Inflammatory
:Inflammatory rdf:type owl:Class ;
              rdfs:subClassOf :HealingStage ;
              owl:disjointWith :Proliferative ,
                               :Remodeling .


###  http://www.semanticweb.org/hafsa/ontologies/2025/10/wound-ontology/NewPatient
:NewPatient rdf:type owl:Class ;
            rdfs:subClassOf :Patient .


###  http://www.semanticweb.org/hafsa/ontologies/2025/10/wound-ontology/Patient
:Patient rdf:type owl:Class .


###  http://www.semanticweb.org/hafsa/ontologies/2025/10/wound-ontology/PhysicalInjury
:PhysicalInjury rdf:type owl:Class ;
                rdfs:subClassOf :Cause .


###  http://www.semanticweb.org/hafsa/ontologies/2025/10/wound-ontology/Proliferative
:Proliferative rdf:type owl:Class ;
               rdfs:subClassOf :HealingStage ;
               owl:disjointWith :Remodeling .


###  http://www.semanticweb.org/hafsa/ontologies/2025/10/wound-ontology/Remodeling
:Remodeling rdf:type owl:Class ;
            rdfs:subClassOf :HealingStage .


###  http://www.semanticweb.org/hafsa/ontologies/2025/10/wound-ontology/SurgicalCare
:SurgicalCare rdf:type owl:Class ;
              rdfs:subClassOf :Treatment .


###  http://www.semanticweb.org/hafsa/ontologies/2025/10/wound-ontology/SurgicalWound
:SurgicalWound rdf:type owl:Class ;
               rdfs:subClassOf :Wound .


###  http://www.semanticweb.org/hafsa/ontologies/2025/10/wound-ontology/Treatment
:Treatment rdf:type owl:Class ;
           rdfs:subClassOf [ rdf:type owl:Restriction ;
                             owl:onProperty :needsCheck ;
                             owl:someValuesFrom :Wound
                           ] .


###  http://www.semanticweb.org/hafsa/ontologies/2025/10/wound-ontology/Wound
:Wound rdf:type owl:Class ;
       rdfs:subClassOf [ rdf:type owl:Restriction ;
                         owl:onProperty :affects ;
                         owl:someValuesFrom :Patient
                       ] .


