@prefix : <http://www.semanticweb.org/hafsa/ontologies/2025/10/wound-ontology/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.semanticweb.org/hafsa/ontologies/2025/10/wound-ontology/> .

<http://www.semanticweb.org/hafsa/ontologies/2025/10/wound-ontology/> rdf:type owl:Ontology .

#################################################################
# Object Properties
#################################################################

:affects rdf:type owl:ObjectProperty ;
         rdfs:domain :Wound ;
         rdfs:range :Patient .

:hasCause rdf:type owl:ObjectProperty ;
          rdfs:domain :Wound ;
          rdfs:range :Cause .

:healingStage rdf:type owl:ObjectProperty ;
              rdfs:domain :Wound ;
              rdfs:range :HealingStage .

:treatedBy rdf:type owl:ObjectProperty ;
           rdfs:domain :HealingStage ;
           rdfs:range :Treatment .

:needsCheck rdf:type owl:ObjectProperty ;
            rdfs:domain :Treatment ;
            rdfs:range :Wound .

#################################################################
# Data Properties
#################################################################

:patientName rdf:type owl:DatatypeProperty ;
             rdfs:domain :Patient ;
             rdfs:range xsd:string .

:causeInfo rdf:type owl:DatatypeProperty ;
           rdfs:domain :Cause ;
           rdfs:range xsd:string .

:woundID rdf:type owl:DatatypeProperty ;
         rdfs:domain :Wound ;
         rdfs:range xsd:int .

:severityScore rdf:type owl:DatatypeProperty ;
               rdfs:domain :Wound ;
               rdfs:range xsd:int .

:sizeCM rdf:type owl:DatatypeProperty ;
        rdfs:domain :Wound ;
        rdfs:range xsd:float .

:stageTitle rdf:type owl:DatatypeProperty ;
            rdfs:domain :HealingStage ;
            rdfs:range xsd:string .

:stageDurationDays rdf:type owl:DatatypeProperty ;
                   rdfs:domain :HealingStage ;
                   rdfs:range xsd:int .

:treatmentTitle rdf:type owl:DatatypeProperty ;
                rdfs:domain :Treatment ;
                rdfs:range xsd:string .

#################################################################
# Classes and Subclasses (taxonomy)
#################################################################

:Wound rdf:type owl:Class .
:AcuteWound rdf:type owl:Class ; rdfs:subClassOf :Wound .
:ChronicWound rdf:type owl:Class ; rdfs:subClassOf :Wound .
:SurgicalWound rdf:type owl:Class ; rdfs:subClassOf :Wound .

:Patient rdf:type owl:Class .
:ExistingCondition rdf:type owl:Class ; rdfs:subClassOf :Patient .
:NewPatient rdf:type owl:Class ; rdfs:subClassOf :Patient .

:HealingStage rdf:type owl:Class .
:Inflammatory rdf:type owl:Class ; rdfs:subClassOf :HealingStage .
:Proliferative rdf:type owl:Class ; rdfs:subClassOf :HealingStage .
:Remodeling rdf:type owl:Class ; rdfs:subClassOf :HealingStage .

:Treatment rdf:type owl:Class .
:CleaningCare rdf:type owl:Class ; rdfs:subClassOf :Treatment .
:DressingCare rdf:type owl:Class ; rdfs:subClassOf :Treatment .
:SurgicalCare rdf:type owl:Class ; rdfs:subClassOf :Treatment .

:Cause rdf:type owl:Class .
:PhysicalInjury rdf:type owl:Class ; rdfs:subClassOf :Cause .
:ChemicalExposure rdf:type owl:Class ; rdfs:subClassOf :Cause .
:Infection rdf:type owl:Class ; rdfs:subClassOf :Cause .

#################################################################
# Axioms / Restrictions
#################################################################

[ rdf:type owl:AllDisjointClasses ;
  owl:members ( :Wound :Patient :HealingStage :Treatment :Cause )
] .

:Wound rdfs:subClassOf [
    rdf:type owl:Restriction ;
    owl:onProperty :affects ;
    owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    owl:onClass :Patient
] .

:HealingStage rdfs:subClassOf [
    rdf:type owl:Restriction ;
    owl:onProperty :treatedBy ;
    owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    owl:onClass :Treatment
] .

:Treatment rdfs:subClassOf [
    rdf:type owl:Restriction ;
    owl:onProperty :needsCheck ;
    owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    owl:onClass :Wound
] .

#################################################################
# Individuals
#################################################################

# Patients
:patient1 rdf:type :NewPatient ; :patientName "Ahmed" ; rdfs:label "Ahmed" .
:patient2 rdf:type :ExistingCondition ; :patientName "Hiba" ; rdfs:label "Hiba" .
:patient3 rdf:type :NewPatient ; :patientName "Zain" ; rdfs:label "Zain" .

# Causes
:cause1 rdf:type :PhysicalInjury ; :causeInfo "Cut by sharp object" ; rdfs:label "Cut by sharp object" .
:cause2 rdf:type :ChemicalExposure ; :causeInfo "Burn injury" ; rdfs:label "Burn injury" .
:cause3 rdf:type :Infection ; :causeInfo "Bacterial infection" ; rdfs:label "Bacterial infection" .

# Treatments
:treatment1 rdf:type :SurgicalCare ; :treatmentTitle "Daily dressing" ; rdfs:label "Daily dressing" ; :needsCheck :wound1 .
:treatment2 rdf:type :CleaningCare ; :treatmentTitle "Surgical debridement" ; rdfs:label "Surgical debridement" ; :needsCheck :wound2 .
:treatment3 rdf:type :DressingCare ; :treatmentTitle "Wound cover change" ; rdfs:label "Wound cover change" ; :needsCheck :wound3 .

# Healing Stages
:healingStage1 rdf:type :Inflammatory ; :treatedBy :treatment1 ; :stageTitle "Inflammatory" ; :stageDurationDays 4 ; rdfs:label "Inflammatory stage" .
:healingStage2 rdf:type :Proliferative ; :treatedBy :treatment2 ; :stageTitle "Proliferative" ; :stageDurationDays 7 ; rdfs:label "Proliferative stage" .
:healingStage3 rdf:type :Remodeling ; :treatedBy :treatment3 ; :stageTitle "Remodeling" ; :stageDurationDays 10 ; rdfs:label "Remodeling stage" .

# Wounds
:wound1 rdf:type :ChronicWound ; :affects :patient1 ; :hasCause :cause1 ; :healingStage :healingStage1 ; :severityScore 7 ; :sizeCM 5.0 ; :woundID 1001 ; rdfs:label "Wound 1001" .
:wound2 rdf:type :SurgicalWound ; :affects :patient2 ; :hasCause :cause2 ; :healingStage :healingStage2 ; :severityScore 4 ; :sizeCM 3.5 ; :woundID 1002 ; rdfs:label "Wound 1002" .
:wound3 rdf:type :AcuteWound ; :affects :patient3 ; :hasCause :cause3 ; :healingStage :healingStage3 ; :severityScore 5 ; :sizeCM 4.2 ; :woundID 1003 ; rdfs:label "Wound 1003" .
:wound4 rdf:type :ChronicWound ; :affects :patient2 ; :hasCause :cause3 ; :healingStage :healingStage1 ; :severityScore 6 ; :sizeCM 4.8 ; :woundID 1004 ; rdfs:label "Wound 1004" .
