@prefix ex: <http://example.org/nyc/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ex:GovernmentOffice a rdfs:Class .

ex:Mayor a rdfs:Class ;
    rdfs:subClassOf ex:Person .

ex:Person a rdfs:Class .

ex:PoliticalParty a rdfs:Class .

ex:Term a rdfs:Class .

ex:Rudy_Giuliani a ex:Mayor ;
    ex:endDate "2001-12-31"^^xsd:date ;
    ex:heldOffice ex:Office_of_Mayor ;
    ex:partyAffiliation ex:Republican ;
    ex:startDate "1994-01-01"^^xsd:date ;
    ex:succeededBy ex:Michael_Bloomberg .

ex:endDate a rdf:Property .

ex:heldOffice a rdf:Property .

ex:partyAffiliation a rdf:Property .

ex:startDate a rdf:Property .

ex:succeededBy a rdf:Property .

ex:Bill_de_Blasio a ex:Mayor ;
    ex:endDate "2021-12-31"^^xsd:date ;
    ex:heldOffice ex:Office_of_Mayor ;
    ex:partyAffiliation ex:Democratic ;
    ex:startDate "2014-01-01"^^xsd:date ;
    ex:succeededBy ex:Eric_Adams .

ex:Eric_Adams a ex:Mayor ;
    ex:heldOffice ex:Office_of_Mayor ;
    ex:partyAffiliation ex:Democratic ;
    ex:startDate "2022-01-01"^^xsd:date .

ex:Independent a ex:PoliticalParty .

ex:Michael_Bloomberg a ex:Mayor ;
    ex:endDate "2013-12-31"^^xsd:date ;
    ex:heldOffice ex:Office_of_Mayor ;
    ex:partyAffiliation ex:Independent,
        ex:Republican ;
    ex:startDate "2002-01-01"^^xsd:date ;
    ex:succeededBy ex:Bill_de_Blasio .

ex:Democratic a ex:PoliticalParty .

ex:Republican a ex:PoliticalParty .

ex:Office_of_Mayor a ex:GovernmentOffice .

